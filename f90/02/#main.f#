
      PROGRAM main
C-----------------------------------------------------------------------
C***********************************************************************
C.......................................................................
C..............................main.....................................
C......................ZERO DIMENSIONAL REACTOR.........................
C.......................................................................
C.......................................................................
C***********************************************************************
C-----------------------------------------------------------------------
      INCLUDE 'param.h'
	

      real x(np),w
      real(4) ta(2),xt1,xt2
      
      real(4), external :: dtime
      
      integer*4 n0, n1, n2
      
      common /random/ n0,n1,n2
      
      open(unit=161,file="ran_start",status="old")
      read(161,*) n0,n1,n2
      close (161)
      
      open(unit=14,file="out",status="unknown")
            
      do k=1,12*np
         call collision(x,w,1) 
      enddo
      
      xt1=dtime(ta)
      
      do k=1,nr
         call collision(x,w,1)     
      enddo
            
      close (14)
      
      open(unit=161,file="ran_start",status="unknown")
      write(161,*) n0,n1,n2
      close (161)
      
      xt2=dtime(ta)
      print *, xt2
      
      end
